{
  "name": "safer-globe-arms-report",
  "version": "0.1.0",
  "private": true,
  "homepage": "/armsreport",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.5.1",
    "postcss-cli": "^4.0.0",
    "prettier": "^1.3.1",
    "react-scripts": "1.0.7"
  },
  "dependencies": {
    "babyparse": "^0.4.6",
    "d3": "^4.10.0",
    "d3-geo-projection": "^2.2.0",
    "d3-request": "^1.0.5",
    "fontawesome": "^4.7.2",
    "loaders.css": "^0.1.2",
    "material-ui": "^1.0.0-beta.6",
    "material-ui-icons": "^1.0.0-beta.5",
    "npm-font-source-sans-pro": "^1.0.2",
    "patch-package": "^3.5.0",
    "react": "^15.5.4",
    "react-csv": "^1.0.8",
    "react-dom": "^15.5.4",
    "react-intl-universal": "^1.2.1",
    "react-loaders": "^2.5.0",
    "react-markdown": "^2.5.0",
    "react-markdown-it": "^1.0.2",
    "react-router-dom": "^4.1.1",
    "react-share": "^1.16.0",
    "save-svg-as-png": "^1.2.0",
    "source-sans-pro": "^2.0.10",
    "start": "^5.1.0",
    "topojson": "^3.0.0",
    "yarn": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "scripts": {
    "prepare": "patch-package",
    "start": "react-scripts start && postcss -u autoprefixer --no-map -r ./src/styles/*.css",
    "build": "react-scripts build",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "precommit": "lint-staged",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --single-quote --trailing-comma all --write \"{,!(node_modules|build)/**/}*.js*\""
  },
  "browserslist": [
    "last 3 versions"
  ]
}
